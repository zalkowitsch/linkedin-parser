{
  "name": "@zalko/linkedin-parser",
  "version": "1.0.0",
  "description": "LinkedIn resume PDF parser with comprehensive Jest testing and test data generation",
  "main": "dist/server.js",
  "author": {
    "name": "Arkady Zalkowitsch",
    "email": "arkady@zalko.com"
  },
  "license": "MIT",
  "keywords": ["pdf", "parser", "resume", "linkedin", "jest", "testing"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zalkowitsch/linkedin-parser.git"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --config jest.pdf-test.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:e2e": "tsx src/e2e-test.ts",
    "test:pdf": "node test-pdf.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@types/pdf-parse": "^1.1.4",
    "hono": "^4.0.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
